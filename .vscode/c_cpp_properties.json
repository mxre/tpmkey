{
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${workspaceRoot}",
        "${workspaceRoot}/libtpm",
        "/usr/local/include",
        "/usr/include"
      ],
      "defines": [
        "TPM_POSIX=1",
        "TPM_NV_DISK=1",
        "TPM_AES=1",
        "TPM_V12=1",
        "TPM_USE_TAG_IN_STRUCTURE=1",
        "TPM_USE_CHARDEV=1",
        "_GNU_SOURCE=1"
      ],
      "browse": {
        "path": [
          "/usr/local/include",
          "/usr/include"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      },
      "intelliSenseMode": "clang-x64"
    }
  ],
  "version": 4
}
